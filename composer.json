{
    "name": "w3r-one/json-schema-bundle",
    "description": "serialize Symfony Forms into JSON schema",
    "type": "symfony-bundle",
    "keywords": ["json schema", "Symfony", "form"],
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Laurent Bientz",
            "email": "laurent.bientz@w3r.one"
        }
    ],
    "require": {
        "php": ">=7.4",
        "doctrine/inflector": ">=1.4",
        "symfony/config": "^5.4|^6.0",
        "symfony/dependency-injection": "^5.4|^6.0",
        "symfony/form": "^5.4|^6.0",
        "symfony/http-foundation": "^5.4|^6.0",
        "symfony/http-kernel": "^5.4|^6.0",
        "symfony/intl": "^5.4|^6.0",
        "symfony/security-csrf": "^5.4|^6.0",
        "symfony/translation": "^5.4|^6.0"
    },
    "require-dev": {
      "phpunit/phpunit": "^8.0"
    },
    "suggest": {
      "symfony/validator": "For form validation.",
      "symfony/security-core": "For hashing users passwords.",
      "symfony/serializer": "To serialize your initial data."
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
          "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "W3rOne\\JsonSchemaBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "W3rOne\\JsonSchemaBundle\\Tests\\": "tests/"
        }
    }
}
